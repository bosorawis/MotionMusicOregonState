        android:title="@string/pref_right_effect_1_title">
        <com.pavelsikun.seekbarpreference.SeekBarPreference
            android:key="rightEffect_1_volume"
            android:title="@string/pref_right_effect_1_volume"
            android:summary="@string/pref_effect_volume_summary"
            android:enabled="true"
            android:defaultValue="5"
            sample:msbp_minValue="0"
            sample:msbp_maxValue="10"
            sample:msbp_interval="1"/>

        <com.pavelsikun.seekbarpreference.SeekBarPreference
            android:key="rightEffect_1_level"
            android:title="@string/pref_right_effect_1_level"
            android:summary="@string/pref_effect_level_summary"
            android:enabled="true"
            android:defaultValue="5"
            sample:msbp_minValue="0"
            sample:msbp_maxValue="10"
            sample:msbp_interval="1"/>
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_right_effect_2_title">
        <com.pavelsikun.seekbarpreference.SeekBarPreference
            android:key="rightEffect_2_volume"
            android:title="@string/pref_right_effect_2_volume"
            android:summary="@string/pref_effect_volume_summary"
            android:enabled="true"
            android:defaultValue="5"
            sample:msbp_minValue="0"
            sample:msbp_maxValue="10"
            sample:msbp_interval="1"/>

        <com.pavelsikun.seekbarpreference.SeekBarPreference
            android:key="rightEffect_2_level"
            android:title="@string/pref_right_effect_2_level"
            android:summary="@string/pref_effect_level_summary"
            android:enabled="true"
            android:defaultValue="5"
            sample:msbp_minValue="0"
            sample:msbp_maxValue="10"
            sample:msbp_interval="1"/>
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_right_effect_3_title">
        <com.pavelsikun.seekbarpreference.SeekBarPreference
            android:key="rightEffect_3_volume"
            android:title="@string/pref_right_effect_3_volume"
            android:summary="@string/pref_effect_volume_summary"
            android:enabled="true"
            android:defaultValue="5"
            sample:msbp_minValue="0"
            sample:msbp_maxValue="10"
            sample:msbp_interval="1"/>

        <com.pavelsikun.seekbarpreference.SeekBarPreference
            android:key="rightEffect_3_level"
            android:title="@string/pref_right_effect_3_level"
            android:summary="@string/pref_effect_level_summary"
            android:enabled="true"
            android:defaultValue="5"
            sample:msbp_minValue="0"
            sample:msbp_maxValue="10"
            sample:msbp_interval="1"/>
    </PreferenceCategory>
    <!--******************************************************-->

</PreferenceScreen>